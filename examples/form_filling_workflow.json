{
  "id": "form_filling_workflow",
  "name": "Web Form Filling Workflow",
  "description": "Automated form filling using browser automation",
  "version": "1.0",
  "created_at": "2025-10-29T00:00:00Z",
  "actions": [
    {
      "step": 1,
      "type": "browser_navigate",
      "description": "Navigate to form page",
      "url": "https://example.com/contact-form",
      "wait_until": "networkidle"
    },
    {
      "step": 2,
      "type": "browser_fill_form",
      "description": "Fill contact form",
      "form_data": {
        "#name": "John Doe",
        "#email": "john.doe@example.com",
        "#subject": "Inquiry about AGI Assistant",
        "#message": "I would like to learn more about your automation capabilities."
      }
    },
    {
      "step": 3,
      "type": "browser_check",
      "description": "Accept terms and conditions",
      "selector": "#terms"
    },
    {
      "step": 4,
      "type": "browser_screenshot",
      "description": "Capture filled form",
      "full_page": false
    },
    {
      "step": 5,
      "type": "browser_submit_form",
      "description": "Submit the form",
      "form_selector": "#contact-form"
    },
    {
      "step": 6,
      "type": "wait",
      "description": "Wait for submission confirmation",
      "duration": 3.0
    },
    {
      "step": 7,
      "type": "browser_screenshot",
      "description": "Capture confirmation page"
    }
  ],
  "metadata": {
    "category": "form_automation",
    "tags": ["browser", "form", "submission"],
    "estimated_duration": 20,
    "requires_browser": true
  }
}
